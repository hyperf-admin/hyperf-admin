{
    "name": "hyperf-admin/base-utils",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "daodao97",
            "email": "daodao97@foxmail.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "ext-json": "*",
        "ext-pdo": "*",
        "ext-swoole": ">=4.4",
        "ext-yaml": "*",
        "aliyuncs/oss-sdk-php": "^2.3",
        "box/spout": "^3.1",
        "hyperf/amqp": "~2.0.0",
        "hyperf/cache": "~2.0.0",
        "hyperf/command": "~2.0.0",
        "hyperf/config": "~2.0.0",
        "hyperf/constants": "~2.0.0",
        "hyperf/database": "~2.0.0",
        "hyperf/db-connection": "~2.0.0",
        "hyperf/filesystem": "~2.0.0",
        "hyperf/framework": "~2.0.0",
        "hyperf/guzzle": "~2.0.0",
        "hyperf/http-server": "~2.0.0",
        "hyperf/logger": "~2.0.0",
        "hyperf/memory": "~2.0.0",
        "hyperf/metric": "~2.0.0",
        "hyperf/nsq": "~2.0.0",
        "hyperf/process": "~2.0.0",
        "hyperf/redis": "~2.0.0",
        "hyperf/snowflake": "~2.0.0",
        "yadakhov/insert-on-duplicate-key": "^1.2"
    },
    "require-dev": {
        "swoft/swoole-ide-helper": "^4.2",
        "phpstan/phpstan": "^0.11.2",
        "hyperf/devtool": "~2.0.0",
        "hyperf/testing": "~2.0.0",
        "daodao97/hyperf-watch": "dev-master",
        "symfony/var-dumper": "^5.0"
    },
    "suggest": {
        "ext-openssl": "Required to use HTTPS.",
        "ext-json": "Required to use JSON.",
        "ext-pdo": "Required to use MySQL Client.",
        "ext-pdo_mysql": "Required to use MySQL Client.",
        "ext-redis": "Required to use Redis Client."
    },
    "autoload": {
        "psr-4": {
            "HyperfAdmin\\BaseUtils\\": "src/"
        },
        "files": [
            "src/Helper/constants.php",
            "src/Helper/common.php",
            "src/Helper/array.php",
            "src/Helper/system.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "HyperfTest\\": "./tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "hyperf": {
            "config": "HyperfAdmin\\BaseUtils\\ConfigProvider"
        }
    }
}
